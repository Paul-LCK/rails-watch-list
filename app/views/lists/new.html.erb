<div class="container">
  <div class="row">
    <div class="col">
      <h1>Add a List</h1>
        <%= simple_form_for(@list) do |f| %>
        <%= f.input :name %>
        <%= f.submit "Submit comment", class: "btn btn-success" %>
        <% end %>
        <%= link_to 'Back to the Lists', lists_path, class: "btn btn-success" %>
    </div>
  </div>
</div>
